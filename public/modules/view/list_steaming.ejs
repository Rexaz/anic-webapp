<!-- Controller for angular -->
<div class="" ng-controller="ListSteamingController">
  <h1>List Steaming</h1>

  <div class="" ng-repeat="data in list_steaming"><!-- รับข้อมูลจาก controller ตัวแปล $scope.list_steaming มา repeat -->
    <div class="">
      <h3>Topic</h3>
      <a ui-sref="Steaming?topic{{data.topic}}"><h4>{{data.topic}}</h4></a>
    </div>

    <div class="">
      <h3>Status</h3>
      <h4>N/A</h4>
    </div>

    <div class="">
      <h3>Last update</h3>
      <h4>{{data.last_update}}</h4>
    </div>
    <br>
    <hr>
    <br>

  </div>


</div>
